{
  "name": "ReactNativeCliAtomicBoilerplate",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "android-storybook": "react-native run-android --variant=storybook",
    "android-local": "react-native run-android --variant=local",
    "android-staging": "react-native run-android --variant=staging",
    "android-production": "react-native run-android --variant=production",
    "ios": "react-native run-ios --scheme \"ReactNativeCliAtomicBoilerplateLocal\"",
    "ios-storybook": "react-native run-ios --scheme \"ReactNativeCliAtomicBoilerplateStorybook\"",
    "ios-staging": "react-native run-ios --scheme \"ReactNativeCliAtomicBoilerplateStaging\"",
    "ios-production": "react-native run-ios --scheme \"ReactNativeCliAtomicBoilerplateProduction\"",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "prestorybook": "rnstl",
    "storybook": "start-storybook -p 7007"
  },
  "dependencies": {
    "@react-navigation/native": "^6.0.11",
    "@react-navigation/native-stack": "^6.7.0",
    "@storybook/react-native": "^5.3.25",
    "react": "18.0.0",
    "react-native": "0.69.1",
    "react-native-config": "^1.4.6",
    "react-native-safe-area-context": "^4.3.1",
    "react-native-screens": "^3.15.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/runtime": "^7.12.5",
    "@react-native-community/eslint-config": "^2.0.0",
    "@storybook/addon-actions": "^6.5.9",
    "@storybook/addon-controls": "^6.5.9",
    "@storybook/addon-ondevice-actions": "^5.3.23",
    "@storybook/addon-ondevice-knobs": "^5.3.25",
    "@storybook/react-native-server": "^5.3.23",
    "@testing-library/react-native": "^10.1.1",
    "@tsconfig/react-native": "^2.0.0",
    "@types/jest": "^26.0.23",
    "@types/react-native": "^0.69.1",
    "@types/react-test-renderer": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.29.0",
    "@typescript-eslint/parser": "^5.29.0",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.2.5",
    "eslint": "^7.32.0",
    "jest": "^26.6.3",
    "metro-react-native-babel-preset": "^0.70.3",
    "react-dom": "18.0.0",
    "react-native-storybook-loader": "^2.0.5",
    "react-test-renderer": "18.0.0",
    "typescript": "^4.4.4"
  },
  "resolutions": {
    "@types/react": "^18"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "config": {
    "react-native-storybook-loader": {
      "searchDir": [
        "./src/components"
      ],
      "pattern": "**/*.stories.tsx",
      "outputFile": "./storybook/storyLoader.js"
    }
  }
}
